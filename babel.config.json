{
    "presets": [
        ["@babel/preset-env",{
            "targets": {
                "node": "current"
            }
        }]
    ],
    "plugins": [
      ["@babel/transform-runtime", {
          "loose": true,
          "modules": false,
          "targets": {
            "esmodules": true
          }
      }]
    ]
}
